ow immediately if user has seen content before\r\n                document.getElementById('delayed-content').style.display = 'block';\r\n                initAllScripts();\r\n            } else {\r\n                \/\/ Wait 15 minutes then show and save flag\r\n                setTimeout(() => {\r\n                    document.getElementById('delayed-content').style.display = 'block';\r\n                    localStorage.setItem('hasSeenContent', 'true');\r\n                    initAllScripts();\r\n                }, delayTime);\r\n            }\r\n        });\r\n\r\n        \/\/ Function to initialize all other scripts once content is visible\r\n        function initAllScripts() {\r\n            lucide.createIcons();\r\n            startTimer();\r\n            startConnectionAnimation();\r\n            startToasts();\r\n            generateProtocol();\r\n            initAccordion();\r\n        }\r\n\r\n        \/\/ --- 2. TIMER LOGIC ---\r\n        function startTimer() {\r\n            let timeLeft = 300; \/\/ 5 minutes\r\n            const timerDisplay = document.getElementById('timer-display');\r\n            \r\n            const timer = setInterval(() => {\r\n                if (timeLeft <= 0) {\r\n                    clearInterval(timer);\r\n                    timeLeft = 0;\r\n                } else {\r\n                    timeLeft--;\r\n                }\r\n                \r\n                const mins = Math.floor(timeLeft \/ 60);\r\n                const secs = timeLeft % 60;\r\n                timerDisplay.textContent = `${mins.toString().padStart(2, '0')}:${secs.toString().padStart(2, '0')}`;\r\n            }, 1000);\r\n        }\r\n\r\n        \/\/ --- 3. CONNECTION ANIMATION ---\r\n        function startConnectionAnimation() {\r\n            const statusTexts = [\"Conex\u00e3o Segura SSL\", \"Validando Chave Pix\", \"Aguardando Confirma\u00e7\u00e3o\"];\r\n            let currentIndex = 0;\r\n            const textElement = document.getElementById('connection-text');\r\n\r\n            setInterval(() => {\r\n                currentIndex = (currentIndex + 1) % statusTexts.length;\r\n                textElement.textContent = statusTexts[currentIndex];\r\n            }, 3000);\r\n        }\r\n\r\n        \/\/ --- 4. RANDOM PROTOCOL ---\r\n        function generateProtocol() {\r\n            const protocol = Math.floor(100000 + Math.random() * 900000);\r\n            document.getElementById('protocol-number').textContent = `${protocol}-B`;\r\n        }\r\n\r\n        \/\/ --- 5. TOAST LOGIC ---\r\n        function startToasts() {\r\n            const notifications = [\r\n                { name: \"Joana M.\", value: \"1.417,00\" },\r\n                { name: \"Carlos E.\", value: \"2.890,50\" },\r\n                { name: \"Fernanda S.\", value: \"950,00\" },\r\n                { name: \"Roberto A.\", value: \"1.200,00\" },\r\n                { name: \"Amanda T.\", value: \"3.150,00\" }\r\n            ];\r\n\r\n            const toast = document.getElementById('toast');\r\n            const toastMessage = document.getElementById('toast-message');\r\n\r\n            function showRandomToast() {\r\n                const notif = notifications[Math.floor(Math.random() * notifications.length)];\r\n                toastMessage.textContent = `${notif.name} recebeu R$ ${notif.value}`;\r\n                \r\n                \/\/ Show\r\n                toast.classList.remove('translate-x-20', 'opacity-0');\r\n                toast.classList.add('translate-x-0', 'opacity-100');\r\n\r\n                \/\/ Hide after 4s\r\n                setTimeout(() => {\r\n                    toast.classList.remove('translate-x-0', 'opacity-100');\r\n                    toast.classList.add('translate-x-20', 'opacity-0');\r\n                }, 4000);\r\n            }\r\n\r\n            setInterval(showRandomToast, 10000); \/\/ Every 10 seconds\r\n        }\r\n\r\n        \/\/ --- 6. ACCORDION LOGIC ---\r\n        function initAccordion() {\r\n            const btns = document.querySelectorAll('.faq-btn');\r\n            \r\n            btns.forEach(btn => {\r\n                btn.addEventListener('click', () => {\r\n                    const content = btn.nextElementSibling;\r\n                    const icon = btn.querySelector('.icon-chevron');\r\n                    const isHidden = content.classList.contains('hidden');\r\n                    \r\n                    \/\/ Close all\r\n                    document.querySelectorAll('.faq-content').forEach(el => el.classList.add('hidden'));\r\n                    document.querySelectorAll('.icon-chevron').forEach(el => {\r\n                        el.setAttribute('data-lucide', 'chevron-down');\r\n                        \/\/ Re-render icon because we changed attribute manually (simplified: just keep chevron-down for simplicity or toggle logic properly)\r\n                        \/\/ For pure JS simplicity, let's just rotate the icon using CSS or toggle classes if we were using raw SVG, \r\n                        \/\/ but with Lucide script, re-rendering is tricky. \r\n                        \/\/ Simpler approach: toggle hidden class only.\r\n                    });\r\n\r\n                    \/\/ Open clicked if it was closed\r\n                    if (isHidden) {\r\n                        content.classList.remove('hidden');\r\n                    }\r\n                });\r\n            });\r\n        }\r\n    <\/script>\r\n<\/body>\r\n<\/html>\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-6fd8c1e1 elementor-widget elementor-widget-html\" data-id=\"6fd8c1e1\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t\t<!-- Pop\u2011ups de coment\u00e1rios G\u202fCaptchas \u2013 100 entradas \/ 27\u202fmin -->\r\n<style>\r\n  #comment-popups{\r\n    position:fixed;bottom:20px;left:20px;z-index:9999;\r\n    pointer-events:none;font-family:\"Helvetica Neue\",Arial,sans-serif\r\n  }\r\n  .comment-popup{\r\n    